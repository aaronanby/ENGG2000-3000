#Start and Stop?:
const int motorPin = 9;  // Motor control pin

void setup() {
  pinMode(motorPin, OUTPUT);
}

void loop() {
  digitalWrite(motorPin, HIGH); // Motor ON
  delay(5000);                  // Run for 5 seconds

  digitalWrite(motorPin, LOW);  // Motor OFF
  delay(5000);                  // Stay OFF for 5 seconds
}
